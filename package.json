    "eslint": "^4.3.0",
    "eslint-config-moxy": "^4.1.0",
    "husky": "^0.14.3",
    "jest": "^22.0.0",
    "lint-staged": "^6.0.0",
    "yargs-parser": "^8.0.0"
  },
  "dependencies": {
    "flat": "^4.0.0",
    "lodash": "^4.17.4",
    "yargs": "^10.0.3"
