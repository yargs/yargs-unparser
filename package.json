  "keywords": [
    "yargs",
    "unparse",
    "expand",
    "inverse",
    "argv"
  ],
  "author": "Andr√© Cruz <andre@moxy.studio>",
  "homepage": "https://github.com/moxystudio/yargs-unparser",
  "repository": {
    "type": "git",
    "url": "git@github.com:moxystudio/yargs-unparser.git"
  },
  "license": "MIT",
  "main": "index.js",
  "files": [],
  "scripts": {
    "lint": "eslint .",
    "test": "jest --env node --coverage",
    "prerelease": "npm t && npm run lint",
    "release": "standard-version",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS"
